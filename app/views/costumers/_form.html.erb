
<%= form_with model: costumer, local: true do |form|%>

    <div>
    <%= form.label :name%>
    <%= form.text_field :name%>
    </div>

    <div>
    <%= form.label :document, 'CPF'%>
    <%= form.text_field :document%>
    </div>

    <div>
    <%= form.label :email%>
    <%= form.text_field :email %>
    </div>

<%= form.submit 'Enviar'%>
<% end %>